      "config": "rn-cli.config.js"
    },
    "android": {
      "package": "com.tuandn.vcafe",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCNtgt588DW9MK4TmBME6q5T5NhNNLrrKo"
        }
